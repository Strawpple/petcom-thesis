    <script>
      // Show Dashboard
      function showDashboard() {
        var dashboard = document.getElementById("dashboard").value;

        if (dashboard == "dashboard") {
          document.getElementById("dashboard_data").style.display = "block";
          document.getElementById("donation_data").style.display = "none";
          document.getElementById("inventory_data").style.display = "none";
        }
      }
      // Show Report
      function showDonation() {
        var donation = document.getElementById("donation").value;

        if (donation == "donation") {
          document.getElementById("donation_data").style.display = "block";
          document.getElementById("dashboard_data").style.display = "none";
          document.getElementById("inventory_data").style.display = "none";
        }
      }
      // Show Inventory
      function showInventory() {
        var inventory = document.getElementById("inventory").value;

        if (inventory == "inventory") {
          document.getElementById("inventory_data").style.display = "block";
          document.getElementById("dashboard_data").style.display = "none";
          document.getElementById("donation_data").style.display = "none";
        }
      }

      let TypeofDonation = (button) => {
        let element = document.getElementById("typeDescending");
        let hidden = element.getAttribute("hidden");

        let typeascending = document.getElementById("typeAscending");
        let typehidden = typeascending.getAttribute("hidden");

        let ascending = document.getElementById("ascending");
        let descending = document.getElementById("descending");

        if (hidden) {
          element.removeAttribute("hidden");
          typeascending.setAttribute("hidden", "hidden");
          ascending.setAttribute("hidden", "hidden");
          descending.setAttribute("hidden", "hidden");
        } else {
          typeascending.removeAttribute("hidden");
          element.setAttribute("hidden", "hidden");
          ascending.setAttribute("hidden", "hidden");
          descending.setAttribute("hidden", "hidden");
        }
      };

      //Sort Date

      let Desc = (button) => {
        let element = document.getElementById("descending");
        let hidden = element.getAttribute("hidden");

        let ascending = document.getElementById("ascending");
        let hiddenasc = ascending.getAttribute("hidden");

        let typeascending = document.getElementById("typeAscending");
        let typedescending = document.getElementById("typeDescending");

        if (hidden) {
          element.removeAttribute("hidden");
          ascending.setAttribute("hidden", "hidden");
          typeascending.setAttribute("hidden", "hidden");
          typedescending.setAttribute("hidden", "hidden");
        } else {
          ascending.removeAttribute("hidden");
          element.setAttribute("hidden", "hidden");
          typeascending.setAttribute("hidden", "hidden");
          typedescending.setAttribute("hidden", "hidden");
        }
      };


      function filterItems(){
        var x = document.getElementById("filteritem").value;
        // console.log(x)

        let dogfood = document.getElementById("dogfood_inventory");
        let catfood = document.getElementById("catfood_inventory");
        let vitamins = document.getElementById("vitamin_inventory");
        let dewormer = document.getElementById("deworm_inventory");
        let others = document.getElementById("otherdonation_inventory");
        let cash = document.getElementById("cash_inventory");
        let cashtotal = document.getElementById("cash_total");

        // console.log(selectedOption)
        if(x == "Dog Food"){
          dogfood.removeAttribute("hidden");
          catfood.setAttribute("hidden", "hidden");
          vitamins.setAttribute("hidden", "hidden");
          dewormer.setAttribute("hidden", "hidden");
          others.setAttribute("hidden", "hidden");
          cash.setAttribute("hidden", "hidden");
